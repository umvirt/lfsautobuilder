. /cscripts/env.sh

cd /sources

tar -xf elfutils-0.190.tar.bz2

cd elfutils-0.190

./configure --prefix=/usr                \
            --disable-debuginfod         \
            --enable-libdebuginfod=dummy

make

if [[ "$ULFS_TESTS" == "YES" ]]
then
make check
fi



make -C libelf install
install -vm644 config/libelf.pc /usr/lib/pkgconfig
rm /usr/lib/libelf.a


